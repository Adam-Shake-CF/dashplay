{
    "firebase": "dashplay",
    "public": ".",
    "ignore": [
        "firebase.json",
        "server.js",
        "api.go",
        "app.yaml"
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "**",
            "destination": "/dashplay.html"
        }
    ],
    "rules": "rules.json"
}
